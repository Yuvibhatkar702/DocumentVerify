# Production Environment Variables for Backend Service
NODE_ENV=production
PORT=5001

# Database Configuration
MONGODB_URI=your_mongodb_connection_string_here

# JWT Configuration
JWT_SECRET=your_super_secure_jwt_secret_key_here

# AI/ML Service URL
